';Raymond
INPUT ON
NASC 8
ALIGN 7
DIR 1

DIM PARTS$(13)
MARGINX% = 100
TOPY% = 300
MARGINX2% = 400
MARGINX3% = 540
MARGINX4% = 800
LINEHEIGHT% = 60
DIM TITLES$(9)
TITLES$(0) = "箱号PKGID"
TITLES$(1) = "描述DESC"
TITLES$(2) = "编码ITEM"
TITLES$(3) = "数量QTY"
TITLES$(4) = "代码CODE"
TITLES$(5) = "批次号LOT NO"
TITLES$(6) = "日期"
TITLES$(7) = "供应商型号MPN"
TITLES$(8) = "备注"

IN$ = "000000000@1@2@3@4@5@6@7@8@00000 00000@11111 11111@22222 22222@33333 33333"

C%=SPLIT(IN$,"PARTS$",64) :' @=64 AS separator
FONT "FangSong",8,0,100

PRPOS MARGINX%, TOPY%
PRTXT TITLES$(0)
PRPOS MARGINX2%, TOPY%
BARSET "CODE128",2,1,3,96
BARFONT "Univers",6,0,5,1,1,100 ON
PRBAR PARTS$(0)
FOR I% = 1 TO 8
PRPOS MARGINX%, TOPY% - LINEHEIGHT% * I%
PRTXT TITLES$(I%) + ":"
PRPOS MARGINX2%, TOPY%  - LINEHEIGHT% * I%
PRTXT PARTS$(I%)
NEXT
PRPOS MARGINX3%, TOPY% - LINEHEIGHT% * 2
BARSET "QRCODE",1,1,4,2,2
PRBAR PARTS$(9)
PRPOS MARGINX4%, TOPY% - LINEHEIGHT% * 2
BARSET "QRCODE",1,1,4,2,2
PRBAR PARTS$(10)
PRPOS MARGINX3%, TOPY% - LINEHEIGHT% * 5 - 30
BARSET "QRCODE",1,1,4,2,2
PRBAR PARTS$(11)
PRPOS MARGINX4%, TOPY% - LINEHEIGHT% * 5 - 30 
BARSET "QRCODE",1,1,4,2,2
PRBAR PARTS$(12)

PRINT KEY OFF
